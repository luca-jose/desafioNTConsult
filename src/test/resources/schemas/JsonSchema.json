{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "nome",
    "cpf",
    "email",
    "valor",
    "parcelas",
    "seguro"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        53
      ]
    },
    "nome": {
      "type": "string",
      "default": "",
      "title": "The nome Schema",
      "examples": [
        "Lucas"
      ]
    },
    "cpf": {
      "type": "string",
      "default": "",
      "title": "The cpf Schema",
      "examples": [
        "34322222232"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "email@yahoo.com"
      ]
    },
    "valor": {
      "type": "number",
      "default": 0.0,
      "title": "The valor Schema",
      "examples": [
        2500.00
      ]
    },
    "parcelas": {
      "type": "integer",
      "default": 0,
      "title": "The parcelas Schema",
      "examples": [
        2
      ]
    },
    "seguro": {
      "type": "boolean",
      "default": false,
      "title": "The seguro Schema",
      "examples": [
        true
      ]
    }
  },
  "examples": [{
    "id": 53,
    "nome": "Lucas",
    "cpf": "34322222232",
    "email": "email@yahoo.com",
    "valor": 2500.00,
    "parcelas": 2,
    "seguro": true
  }]
}